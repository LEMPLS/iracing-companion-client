{
  "name": "lempls-iracing-companion-dashboard",
  "version": "1.0.0",
  "description": "LEMPLS iRacing Companion Dashboard",
  "private": true,
  "main": "src/electron-starter.js",
  "contributors": [
    "Martin Kapal",
    "Tomáš Korený"
  ],
  "license": "MIT",
  "homepage": "./",
  "dependencies": {
    "classnames": "^2.2.5",
    "cross-env": "^5.1.4",
    "react": "^16.3.1",
    "react-dom": "^16.3.1",
    "react-redux": "^5.0.7",
    "react-scripts-ts": "2.15.1",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0",
    "sockette": "^2.0.0",
    "typesafe-actions": "^2.0.0-1",
    "utility-types": "^2.0.0"
  },
  "scripts": {
    "start": "cross-env BROWSER=none react-scripts-ts start",
    "build": "react-scripts-ts build",
    "test": "react-scripts-ts test --env=jsdom",
    "eject": "react-scripts-ts eject",
    "electron": "electron .",
    "electron-dev": "cross-env ELECTRON_START_URL=http://localhost:3000 electron .",
    "pack": "electron-builder --dir",
    "dist": "electron-builder"
  },
  "devDependencies": {
    "@types/classnames": "^2.2.3",
    "@types/jest": "^22.2.3",
    "@types/node": "^9.6.5",
    "@types/react": "^16.3.10",
    "@types/react-dom": "^16.0.5",
    "@types/react-redux": "^5.0.16",
    "electron": "^1.8.4",
    "electron-builder": "^20.8.1",
    "npm-run-all": "^4.1.2",
    "prettier": "^1.12.0",
    "typescript": "^2.8.1"
  },
  "build": {
    "productName": "LEMPLS iRacing Companion Dashboard",
    "directories": {
      "buildResources": "build_app"
    },
    "win": {
      "artifactName": "LiRC-Dashboard.exe",
      "target": [
        {
          "target": "portable",
          "arch": [
            "x64"
          ]
        }
      ]
    }
  }
}
